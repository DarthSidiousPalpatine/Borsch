package com.muurr.tfcplusborsch;

import com.dunk.tfc.Core.TFCTabs;
import com.dunk.tfc.Items.ItemDrink;
import com.dunk.tfc.api.TFCFluids;
import com.dunk.tfc.api.TFCItems;
import com.dunk.tfc.api.Enums.EnumFoodGroup;

import net.minecraftforge.fluids.FluidRegistry;

public class ItemSetup extends TFCItems{
	public static void setup()
	{
		borsch = new ItemDrink(1000).setFoodGroup(EnumFoodGroup.Dairy).setCalories(0.584f).setWaterRestoreRatio(1f).setUnlocalizedName("Borsch").setCreativeTab(TFCTabs.TFC_FOODS);
		drinks.add((ItemDrink) borsch);
	}
}
